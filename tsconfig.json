{
  "compilerOptions": {
      "module": "commonjs",
      "lib": [
          "es6"
      ],
      "noImplicitAny": true,
      "noImplicitThis": true,
      "strictFunctionTypes": true,
      "skipLibCheck": true,
      "strictNullChecks": true,
      "declaration": true,
      "noEmit": false,
      "forceConsistentCasingInFileNames": true,
      "outDir": "dist"
  },
  "include": ["index.d.ts", "src/**/*.ts"],
  "exclude": ["node_modules", "dist"]
 
}